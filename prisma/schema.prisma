generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model prodect {
  id        String   @id @default(cuid())
  name      String
  description String
  featured  Boolean
  price     Int
  image     String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  clrekId   String
}
